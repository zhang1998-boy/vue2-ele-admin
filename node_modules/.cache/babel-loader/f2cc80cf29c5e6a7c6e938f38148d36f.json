mport headTop from \"@/components/headTop\";\r\nimport { goodsDataTableHeader } from '@/utils/tableHeader'\r\nimport { achievementImport } from '@/api/list'\r\nexport default {\r\n  data() {\r\n    return {\r\n      goodsDataTableHeader,\r\n      city: {},\r\n      offset: 0,\r\n      limit: 20,\r\n      count: 0,\r\n      tableData: [],\r\n      currentPage: 1,\r\n      selectTable: {},\r\n      dialogFormVisible: false,\r\n      categoryOptions: [],\r\n      selectedCategory: [],\r\n      address: {},\r\n    }\r\n  },\r\n  created() {\r\n    // this.getResturants();\r\n  },\r\n  components: {\r\n    // headTop,\r\n  },\r\n  methods: {\r\n    handleBeforeUpload(file) {\r\n      // 在这里你可以处理文件，比如转换成File对象\r\n      console.log(file) // 这是已经是File对象了\r\n      // 如果你需要将file转成File对象，实际上file已经是File对象了，直接使用即可\r\n      // 如果你需要做其他处理，比如校验等，可以在这里进行\r\n    },\r\n    async handleExceed(e, b) {\r\n      console.log(e, b)\r\n      debugger\r\n      const formData = new FormData()\r\n      formData.append('file', e.file)\r\n      formData.append('type', 1)\r\n      const restaurants = await achievementImport(formData)\r\n    },\r\n    // 获取表格数据\r\n    async getResturants() {\r\n      const { latitude, longitude } = this.city\r\n      const restaurants = await getResturants({\r\n        latitude,\r\n        longitude,\r\n        offset: this.offset,\r\n        limit: this.limit,\r\n      })\r\n      this.tableData = []\r\n      restaurants.forEach((item) => {\r\n        const tableData = {}\r\n        tableData.name = item.name\r\n        tableData.address = item.address\r\n        tableData.description = item.description\r\n        tableData.id = item.id\r\n        tableData.phone = item.phone\r\n        tableData.rating = item.rating\r\n        tableData.recent_order_num = item.recent_order_num\r\n        tableData.category = item.category\r\n        tableData.image_path = item.image_path\r\n        this.tableData.push(tableData)\r\n      })\r\n    },\r\n\r\n    async onExportXls() {\r\n      const title = `【A1】业务数据${getTime()}`\r\n      const realUrl = expUrl.value\r\n      return handleExportXls(title, realUrl)\r\n    },\r\n\r\n    //分页部分\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`)\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.offset = (val - 1) * this.limit\r\n      this.getResturants()\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\">\r\n// @import \"../../style/mixin\";\r\n.demo-table-expand {\r\n  font-size: 0;\r\n}\r\n.demo-table-expand label {\r\n  width: 90px;\r\n  color: #99a9bf;\r\n}\r\n.demo-table-expand .el-form-item {\r\n  margin-right: 0;\r\n  margin-bottom: 0;\r\n  width: 50%;\r\n}\r\n.table_container {\r\n  padding: 20px;\r\n  height: 90vh;\r\n  background-color: #fff;\r\n}\r\n.Pagination {\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  margin-top: 8px;\r\n}\r\n\r\n.info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  .rt-title {\r\n    color: #000000;\r\n    font-family: PingFang SC;\r\n    font-weight: 600;\r\n    font-size: 14px;\r\n    line-height: 20px;\r\n    letter-spacing: -1px;\r\n  }\r\n  .info-btn {\r\n    display: flex;\r\n  }\r\n}\r\n.right-btn {\r\n  margin-left: 12px;\r\n}\r\n</style>\r\n"],"mappings":";AA6DA;AACA,SAAAA,oBAAA;AACA,SAAAC,iBAAA;AACA;EACAC,KAAA;IACA;MACAF,oBAAA;MACAG,IAAA;MACAC,MAAA;MACAC,KAAA;MACAC,KAAA;MACAC,SAAA;MACAC,WAAA;MACAC,WAAA;MACAC,iBAAA;MACAC,eAAA;MACAC,gBAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA;EAAA,CACA;EACAC,UAAA;IACA;EAAA,CACA;EACAC,OAAA;IACAC,mBAAAC,IAAA;MACA;MACAC,OAAA,CAAAC,GAAA,CAAAF,IAAA;MACA;MACA;IACA;IACA,MAAAG,aAAAC,CAAA,EAAAC,CAAA;MACAJ,OAAA,CAAAC,GAAA,CAAAE,CAAA,EAAAC,CAAA;MACA;MACA,MAAAC,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,SAAAJ,CAAA,CAAAJ,IAAA;MACAM,QAAA,CAAAE,MAAA;MACA,MAAAC,WAAA,SAAA1B,iBAAA,CAAAuB,QAAA;IACA;IACA;IACA,MAAAI,cAAA;MACA;QAAAC,QAAA;QAAAC;MAAA,SAAA3B,IAAA;MACA,MAAAwB,WAAA,SAAAC,aAAA;QACAC,QAAA;QACAC,SAAA;QACA1B,MAAA,OAAAA,MAAA;QACAC,KAAA,OAAAA;M