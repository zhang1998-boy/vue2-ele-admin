, 'warn'].forEach((type) => {\r\n  // 处理message[type](config)参数为对象类型\r\n  dtMessage[type] = (content, duration, onClose) => {\r\n    if (typeof content === 'object') {\r\n      return message[type]({ ...content, content: h => createElementVNode(h, content?.content) });\r\n    }\r\n    return message[type](h => createElementVNode(h, content), duration, onClose);\r\n  };\r\n});\r\nexport default dtMessage;\r\n"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AACA;AACA,SAASA,OAAO,QAAQ,gBAAgB;AACxC;AACA;AACA;AACA;AACA,MAAMC,kBAAkB,GAAGA,CAACC,CAAC,EAAEC,OAAO,KAAK;EACzC;EACA,MAAMC,SAAS,GAAGF,CAAC,CAAC,MAAM,EAAE;IAAEG,KAAK,EAAE;MAAEC,KAAK,EAAE,SAAS;MAAEC,QAAQ,EAAE;IAAO;EAAE,CAAC,EAAEJ,OAAO,CAAC;EACvF;EACA,MAAMK,SAAS,GAAGN,CAAC,CAAC,QAAQ,EAAE;IAC5BG,KAAK,EAAE;MAAEI,UAAU,EAAE,MAAM;MAAEH,KAAK,EAAE,SAAS;MAAEI,MAAM,EAAE;IAAU,CAAC;IAClEC,KAAK,EAAE;MAAEC,IAAI,EAAE;IAAe,CAAC;IAC/BC,EAAE,EAAE;MAAEC,KAAK,EAAEA,CAAA,KAAMd,OAAO,CAACe,OAAO,CAAC;IAAE;EACvC,CAAC,CAAC;EACF;EACA,MAAMC,SAAS,GAAGd,CAAC,CAAC,MAAM,EAAE,CAAC,CAAC,EAAE,CAACE,SAAS,EAAEI,SAAS,CAAC,CAAC;EACvD,OAAOQ,SAAS;AAClB,CAAC;AACD;AACA,MAAMC,SAAS,GAAG,CAAC,CAAC;AACpB;AACA,CAAC,OAAO,EAAE,SAAS,EAAE,MAAM,EAAE,SAAS,EAAE,MAAM,CAAC,CAACC,OAAO,CAAEN,IAAI,IAAK;EAChE;EACAK,SAAS,CAACL,IAAI,CAAC,GAAG,CAACT,OAAO,EAAEgB,QAAQ,EAAEC,OAAO,KAAK;IAChD,IAAI,OAAOjB,OAAO,KAAK,QAAQ,EAAE;MAC/B,OAAOH,OAAO,CAACY,IAAI,CAAC,CAAC;QAAE,GAAGT,OAAO;QAAEA,OAAO,EAAED,CAAC,IAAID,kBAAkB,CAACC,CAAC,EAAEC,OAAO,aAAPA,OAAO,uBAAPA,OAAO,CAAEA,OAAO;MAAE,CAAC,CAAC;IAC7F;IACA,OAAOH,OAAO,CAACY,IAAI,CAAC,CAACV,CAAC,IAAID,kBAAkB,CAACC,CAAC,EAAEC,OAAO,CAAC,EAAEgB,QAAQ,EAAEC,OAAO,CAAC;EAC9E,CAAC;AACH,CAAC,CAAC;AACF,eAAeH,SAAS"}]}